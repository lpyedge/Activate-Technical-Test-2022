<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="renderer" content="webkit" />
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1,maximum-scale =1,user-scalable=no" />
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="full-screen" content="true" />
<meta name="x5-fullscreen" content="true" />
<meta name="360-fullscreen" content="true" />

<meta http-equiv="Cache-Control" content="no-transform" />
<meta http-equiv="Cache-Control" content="no-siteapp">

<script src="//cdn.staticfile.org/jquery/3.5.1/jquery.min.js"
        asp-fallback-test="window.jQuery"
        asp-fallback-src="~/lib/jquery/dist/jquery.min.js"></script>

<link href="https://cdn.staticfile.org/font-awesome/5.15.4/css/all.min.css" rel="stylesheet" type="text/css"
      asp-fallback-test-class="fa-pull-left"
      asp-fallback-test-property="float"
      asp-fallback-test-value="left"
      asp-fallback-href="https://lib.baomitu.com/font-awesome/5.15.4/css/all.min.css">

<script src="//lib.baomitu.com/react/18.2.0/umd/react.production.min.js"
        asp-fallback-test="window.React"
        asp-fallback-src="~/lib/react/umd/react.production.min.js"></script>

<script src="//lib.baomitu.com/react-dom/18.2.0/umd/react-dom.production.min.js"
        asp-fallback-test="window.ReactDOM"
        asp-fallback-src="~/lib/react-dom/umd/react-dom.production.min.js"></script>

<environment include="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.EmailService.js" type="text/javascript" asp-append-version="true"></script>
</environment>

<environment exclude="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.EmailService.js" type="text/javascript" asp-append-version="true"></script>
</environment>

<title>@SiteContext.Config.SiteName</title>
<meta name="Description" content="@Html.Raw(SiteContext.Config.SiteMemo)" />

<script>
    $("html").css("font-size", window.innerWidth < 540 ? window.innerWidth / 10 + "px" : "54px");

    CY.AjaxUrl = "/api";
    CY.MVC = true;
    CY.AjaxMask = false;
    
    $(function () {
        $.emailservice('@SiteContext.Config.ServiceEmail');
    });
</script>