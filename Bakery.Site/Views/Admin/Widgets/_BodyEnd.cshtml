        asp-fallback-test="window.Quasar"
        asp-fallback-src="~/lib/quasar/dist/quasar.umd.min.js"></script>

<script>
    
    function AdminApi(action, data, postForm) {
        if (postForm) {
            var formData = new FormData();
            for (name in data) {
                formData.append(name, data[name]);
            }
            data = formData;
        }
        return axios({
            'method': 'post',
            'headers': {
                'AdminToken': Store.Token()
            },
            'url': '/ApiAdmin/' + action,
            'data': data
        }
        );
    }

    var Notify = {};
    Notify.Warning = function (message) {
        Quasar.Notify.create({
            type: 'warning',
            position: 'center',
            icon: 'warning',
            timeout: 2500,
            message
        });
    }
    Notify.Info = function (message) {
        Quasar.Notify.create({
            type: 'positive',
            position: 'center',
            icon: 'check_circle',
            timeout: 1200,
            message
        });
    }

    function Goto(url, query) {
        if (query) {
            url += '?' + query;
        }
        location.href = url;
    }

</script>